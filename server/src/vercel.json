{
  "version": 2,
  "outputDirectory": "build",
  "buildCommand": "tsc && node ./build/index.js",
  "public":true,
  "build": [
    {
      "src": "./router/UserRouter.ts",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/.*",
      "dest": "./router/UserRouter.ts"
    }
  ]
}
